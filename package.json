{
  "name": "vue-linkify-vue3",
  "version": "2.0.0",
  "description": "A simple Vue directive to turn URL's and emails into clickable links. Maintained fork with Vue 2.7+ and Vue 3 support.",
  "main": "dist/vue-linkify.min.js",
  "scripts": {
    "lint": "eslint index.js",
    "build": "eslint index.js && BABEL_ENV=production && babel index.js -o ./dist/vue-linkify.min.js",
    "test": "npm build && karma start test/unit/karma.conf.js --single-run"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue3",
    "component",
    "directive",
    "url",
    "urlify",
    "link",
    "linkify"
  ],
  "author": "ali <shekhaliul44@gmail.com>",
  "contributors": [
    "Phan An <me@phanan.net> (http://phanan.net) (Original Author)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shekh/vue-linkify.git"
  },
  "bugs": {
    "url": "https://github.com/shekh/vue-linkify/issues"
  },
  "homepage": "https://github.com/shekh/vue-linkify#readme",
  "license": "MIT",
  "dependencies": {
    "linkifyjs": "^4.1.3"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "chai": "^4.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "karma": "^6.4.4",
    "karma-mocha": "^2.0.1",
    "karma-chrome-launcher": "^3.2.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.1",
    "mocha": "^10.7.3",
    "sinon": "^18.0.0",
    "sinon-chai": "^3.7.0",
    "vue": "^3.5.12",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
